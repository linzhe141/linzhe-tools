{
  "name": "linzhe-tools",
  "type": "module",
  "main": "./dist/cjs/linzhe-tools/index.js",
  "module": "./dist/es/linzhe-tools/index.js",
  "types": "./dist/types/index.d.ts",
  "version": "0.0.4",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/es/linzhe-tools/index.js",
      "require": "./dist/cjs/linzhe-tools/index.js"
    },
    "./style.css": {
      "import": "./dist/style/style.css",
      "require": "./dist/style/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "description": "基于vue3的UI工具库",
  "scripts": {
    "publish2npm": "npm version patch && npm publish",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build:src": "rm -rf dist && node script/build.js"
  },
  "keywords": [
    "vue3",
    "seamless scroll",
    "step scroll",
    "number scroll",
    "carousel",
    "scale screen",
    "watermark",
    "无限滚动",
    "分步滚动",
    "数字滚动",
    "轮播",
    "屏幕缩放",
    "水印"
  ],
  "author": "linzhe141@qq.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/linzhe141/linzhe-tools.git"
  },
  "dependencies": {
    "@linzhe-tools/components": "0.0.3",
    "@linzhe-tools/shared": "0.0.1"
  },
  "license": "ISC"
}
