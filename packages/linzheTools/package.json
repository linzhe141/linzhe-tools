{
  "name": "linzhe_tools",
  "type": "module",
  "main": "./dist/cjs/linzheTools/index.js",
  "module": "./dist/es/linzheTools/index.js",
  "version": "1.1.5",
  "exports": {
    ".": {
      "import": "./dist/es/linzheTools/index.js",
      "require": "./dist/cjs/linzheTools/index.js"
    },
    "./style.css": {
      "import": "./dist/style/style.css",
      "require": "./dist/style/style.css"
    }
  },
  "description": "基于vue3的UI工具库",
  "scripts": {
    "publish2npm": "npm version patch && npm publish",
    "build:types": "rm -rf types && vue-tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:src": "rm -rf dist && node script/build.js"
  },
  "keywords": [
    "vue3",
    "seamless scroll",
    "step scroll",
    "number scroll",
    "carousel",
    "scale screen",
    "watermark",
    "无限滚动",
    "分步滚动",
    "数字滚动",
    "轮播",
    "屏幕缩放",
    "水印"
  ],
  "author": "linzhe141@qq.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/linzhe141/linzhe_tools.git"
  },
  "dependencies": {
    "@linzhe_tools/components": "latest",
    "@linzhe_tools/shared": "latest"
  },
  "license": "ISC"
}
